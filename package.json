{
  "name": "clojure-lsp-adapter",
  "main": "./lib/main",
  "version": "0.0.1",
  "description": "Clojure language support for Atom-IDE",
  "keywords": [
    "ide",
    "clojure",
    "atom-ide",
    "autocomplete",
    "languageserver",
    "language-server-protocol",
    "language-client"
  ],
  "repository": "https://github.com/artarf/clojure-lsp-adapter",
  "license": "MIT",
  "engines": {
    "atom": ">=1.38.0 <2.0.0"
  },
  "dependencies": {
    "atom-languageclient": "0.9.9"
  },
  "enhancedScopes": [
    "source.clojure"
  ],
  "activationHooks": [
    "language-clojure:grammar-used",
    "core:loaded-shell-environment"
  ],
  "configSchema": {
    "clojure-lsp": {
      "title": "Clojure Language Server",
      "order": 1,
      "type": "string",
      "default": "clojure-lsp",
      "description": "Path to your clojure-lsp binary used to launch the Clojure language server."
    }
  },
  "consumedServices": {
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    },
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "signature-help": {
      "versions": {
        "0.1.0": "consumeSignatureHelp"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  }
}
